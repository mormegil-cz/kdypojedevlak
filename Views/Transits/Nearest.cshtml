@model IEnumerable<KdyPojedeVlak.Engine.TrainRoutePoint>
@{
    ViewData["Title"] = "Nejbližší vlaky";
}
<h3>Průjezdy vlaku místem „@ViewData["Place"]“</h3>
<ul>
@foreach(var train in Model)
{
    <li>
    @train.AnyScheduledTime
    @Html.ActionLink(train.Train.TrainType + " " + train.Train.TrainNumber + " " + train.Train.TrainName, "Details", "Train", new { id = train.Train.TrainNumber })
    </li>
}
</ul>
